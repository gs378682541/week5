<h1>Meetup App</h1>

<P><%= link_to 'Event',events_url%>|<%= link_to 'Venues',venues_url%></P>

<h1>Listing venues</h1>

<p><%= link_to 'Add a new Venue', new_venue_path %></p>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @venues.each do |venue| %>
  <tr>
    <td><%= link_to venue.name,venue %></td>
    <td><%= venue.address %></td>
    <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
    <td><%= link_to 'Destroy', venue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

